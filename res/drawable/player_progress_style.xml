<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="3.0dip" />
            <gradient android:startColor="#8f979a9e" android:endColor="#8f979a9e" android:centerColor="#8f979a9e" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="3.0dip" />
                <gradient android:startColor="#ffa3c3e7" android:endColor="#ffa3c3e7" android:centerColor="#ffa3c3e7" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="3.0dip" />
                <gradient android:startColor="#ff2ba9d8" android:endColor="#ff2ba9d8" android:centerColor="#ff2ba9d8" />
            </shape>
        </clip>
    </item>
</layer-list>